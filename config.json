{
    "exclude_patterns": [
        "*.log",
        "*.tmp",
        "*.temp",
        "*.swp",
        "*.bak",
        "*.cache",
        "node_modules/",
        ".git/",
        ".svn/",
        "*.pyc",
        "__pycache__/"
    ],
    "max_file_size": 104857600,
    "log_level": "INFO",
    "risk_thresholds": {
        "high": 3,
        "medium": 2,
        "low": 1
    }
}
